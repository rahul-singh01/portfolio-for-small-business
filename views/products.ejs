<div class="page-heading about-heading header-text" style="background-image: url(assets/images/New/Head/head-1.webp);">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="text-content">
                    <h4>Lorem ipsum dolor sit amet</h4>
                    <h2>Products</h2>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="products">
    <div class="container">
        <div class="row">
            <% for(i=0 ; i < data.length ; i++){ %>
                <div class="col-md-4">
                    <div class="product-item">
                        <a href="/productdetails/<%= data[i]._id %>"><img src="<%= data[i].image %>" alt=""></a>
                        <div class="down-content">
                            <a href="/productdetails/<%= data[i]._id %>"">
                                <h4><%= data[i].name %></h4>
                            </a>
                            <h6><small><del>₹ 999.00 </del></small> ₹ <%= data[i].price %></h6>
                            <p><%= data[i].tags.join(' | ') %></p>
                        </div>
                    </div>
                </div>
            <% } %>
        </div>
    </div>
</div>